From: Christopher Harris <xixonia@gmail.com>
Date: Fri, 4 Oct 2024 00:52:17 +0000
Subject: [PATCH] update deps to work with mrc branch-24.06-rapids-24.06

---
 dependencies.yaml | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/dependencies.yaml b/dependencies.yaml
index 0844d86fb6..5eb333002f 100644
--- a/dependencies.yaml
+++ b/dependencies.yaml
@@ -3,7 +3,7 @@ files:
   all:
     output: conda
     matrix:
-      cuda: ["11.8", "12.2"]
+      cuda: ["11.8", "12.4"]
       arch: [x86_64]
     includes:
       - build_base
@@ -242,12 +242,12 @@ dependencies:
           - matrix:
               arch: x86_64
             packages:
-              - gcc_linux-64=11.*
+              - gcc_linux-64=12.1
               - sysroot_linux-64==2.17
           - matrix:
               arch: aarch64
             packages:
-              - gcc_linux-aarch64=11.*
+              - gcc_linux-aarch64=12.1
               - sysroot_linux-aarch64==2.17
       - output_types: conda
         matrices:
@@ -376,9 +376,9 @@ dependencies:
             packages:
               - cuda-version=12.0
           - matrix:
-              cuda: "12.2"
+              cuda: "12.4"
             packages:
-              - cuda-version=12.2
+              - cuda-version=12.4
   cuda:
     specific:
       - output_types: conda
